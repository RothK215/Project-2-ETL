------------ Vegan Restaurants ------------
CREATE TABLE vegan_restaurants(
	id INT PRIMARY KEY,
	name TEXT,
	categories TEXT,
	cuisines TEXT,
	address TEXT,
	postal_code INT,
	city TEXT,
	provice TEXT,
	country TEXT,
	latitude FLOAT,
	longitude FLOAT,
	websites TEXT
	)

